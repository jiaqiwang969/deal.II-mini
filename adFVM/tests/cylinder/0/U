FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{

    right
    {
        type            zeroGradient;
    }

    up
    {
        type            fixedValue;
        value           uniform (100 0 0);
    }

    left
    {
        type            fixedValue;
        value           uniform (100 0 0);
    }

    cylinder
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    z1
    {
        type            cyclic;
    }
    z2
    {
        type            cyclic;
    }

}

// ************************************************************************* //
